<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.common.message">
    <class name="TradeMchtYzm" table="TRADE_MCHT_YZM">
        <id name="id" column="id" type="string">
            <generator class="assigned"/>
        </id>
        <property name="mchtNo" column="mcht_no" type="string"></property>
        <property name="telphone" column="telphone" type="string"></property>
        <property name="yzmType" column="yzm_type" type="string"></property>
        <property name="yzm" column="yzm" type="string"></property>
        <property name="status" column="status" type="string"></property>
        <property name="msg_result" column="msg_result" type="string"></property>
        <property name="insertTime" column="insert_time" type="string"></property>


    </class>
</hibernate-mapping>
